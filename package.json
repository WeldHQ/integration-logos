{
  "name": "@weld/integration-logos",
  "version": "0.1.0",
  "main": "./build/cjs/lib.js",
  "module": "./build/esm/lib.js",
  "types": "./build/types/index.d.ts",
  "exports": {
    "./*.svg": "./integrations/*.svg",
    "./*.png": "./integrations/*.png",
    "./*": {
      "types": "./build/types/*.d.ts",
      "require": "./build/cjs/*.js",
      "import": "./build/esm/*.js",
      "default": "./build/esm/*.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepare": "npm run build",
    "compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "build:config": "tsx ./scripts/generate-logos-config.ts",
    "build:clean": "rm -rf ./build",
    "build": "npm-run-all build:clean build:config compile",
    "dev": "cd configurator && npm run dev"
  },
  "dependencies": {
    "extract-colors": "^4.0.4",
    "get-pixels": "^3.3.3",
    "svg2img": "^1.0.0-beta.2"
  },
  "devDependencies": {
    "@types/get-pixels": "^3.3.4",
    "@types/node": "^20",
    "eslint": "^8",
    "npm-run-all": "^4.1.5",
    "tsx": "^4.9.3",
    "typescript": "^5"
  }
}
